{
  "name": "artemis",
  "version": "0.0.1",
  "private": true,
  "description": "Dashboard & digital Function Sheet replacement",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && cd client && npm test",
    "lint": "eslint --fix --ext .js,.jsx .",
    "prettify": "prettier --write .",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "story": "cd client && npm run storybook",
    "build": "cd client && npm run build && npm run build-storybook",
    "start": "node server.js",
    "postinstall": "cd client && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexisRoe/Artemis.git"
  },
  "keywords": [
    "dashboard",
    "function",
    "sheet",
    "react",
    "mongodb"
  ],
  "author": "Alexis Roehrling",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexisRoe/Artemis/issues"
  },
  "homepage": "https://github.com/AlexisRoe/Artemis#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "nodemon": "^2.0.6",
    "prettier": "2.2.0",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  }
}
